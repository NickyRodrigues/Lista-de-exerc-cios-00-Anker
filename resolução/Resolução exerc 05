using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
        int linhas = 10;
        int colunas = 1;
        
        for(int row = 1; row <= linhas; row++)
        {
            for(int col = 1; col <= colunas; col++)
            {
                int resultado = row * col;
                
                Console.Write($"{row} x {col} = {resultado}\t");
                
            }
            Console.WriteLine();
        }
    }
}
